{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["{projectRoot}/.svelte-kit", "{projectRoot}/dist", "{projectRoot}/build"]
		},
		"cp-env": {
			"dependsOn": ["^cp-env"]
		},
		"lint": {
			"cache": true,
			"dependsOn": ["^build"]
		},
		"dev": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"dev:mock": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"robot": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"robot:mock": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"test": {
			"dependsOn": ["^build"],
			"cache": true
		},
		"check": {
			"cache": true,
			"dependsOn": ["^build"]
		}
	},
	"defaultBase": "main"
}
